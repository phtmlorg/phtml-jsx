{
  "name": "@phtml/jsx",
  "version": "1.0.0",
  "description": "Use JSX in HTML",
  "author": "Jonathan Neal <phtmlorg@hotmail.com>",
  "license": "CC0-1.0",
  "repository": "phtmlorg/phtml-jsx",
  "homepage": "https://github.com/phtmlorg/phtml-jsx#readme",
